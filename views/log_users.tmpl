{{define "logs_users"}}
{{template "top" .Header}}
{{template "menu" .Header}}
<div class="main">
	<div class="main-inner">
		<div class="container">
			<div class="row">
				<div class="span12">
					<div class="widget widget-table action-table">
						<div class="widget-header"> 
							<i class="icon-th-list"></i>
							<h3>User Logs</h3>
						</div>
						<div class="widget-content">
							<table id="example" class="table table-striped table-bordered" style="width:100%">
								<thead>
									<tr>
										<th>Username</th>
										<th>Name</th>
										<th>Date Time</th>
										<th>View</th>
									</tr>
								</thead>
								<tbody>
									{{range .Logs}}
									<tr>
										<td>{{.Username}}</td>
										<td>{{.Name}}</td>
										<td>{{.Time}}</td>
										<td><a href="/logsview?id={{.User_id}}">View</a></td>
									</tr>
									{{end}}
								</tbody>
							</table>
						</div>
            </div></div></div>
</div></div></div>
{{template "bottom"}}
{{end}}